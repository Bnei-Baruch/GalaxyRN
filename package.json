{
  "name": "GalaxyRN",
  "version": "1.0.27",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "clean": "react-native clean",
    "test": "jest",
    "bundle:android": "mkdir -p android/app/src/main/assets && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ --sourcemap-output android/app/build/intermediates/sourcemaps/react/release/index.android.bundle.packager.map",
    "bundle:ios": "mkdir -p ios/main/assets && react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios/main/assets/ --sourcemap-output ios/main.jsbundle.map",
    "sentry:android": "sentry-cli sourcemaps inject --org galaxy --project galaxy-rn --release $npm_package_version ./android/app/src/main/assets/index.android.bundle ./android/app/build/intermediates/sourcemaps/react/release/index.android.bundle.packager.map",
    "sentry:ios": "sentry-cli sourcemaps inject --org galaxy --project galaxy-rn --release $npm_package_version ./ios/main.jsbundle ./ios/main.jsbundle.map",
    "version-bump": "node ./scripts/version-bump.js"
  },
  "dependencies": {
    "@sentry/react-native": "^6.9.0",
    "base-64": "^1.0.0",
    "immer": "^9.0.21",
    "loglevel": "^1.9.1",
    "mqtt": "^5.9.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-app-auth": "^7.2.0",
    "react-native-background-timer": "^2.4.1",
    "react-native-crisp-chat-sdk": "^0.16.1",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-webrtc": "^124.0.3",
    "react-native-webview": "^13.12.3",
    "rn-secure-storage": "^3.0.1",
    "url": "^0.11.4",
    "webrtc-adapter": "^9.0.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@formatjs/intl-pluralrules": "^5.3.4",
    "@react-native-community/cli": "^13.6.9",
    "@react-native-community/cli-platform-android": "^13.6.9",
    "@react-native/babel-preset": "0.74.87",
    "@react-native/eslint-config": "0.74.87",
    "@react-native/metro-config": "0.74.87",
    "@react-native/typescript-config": "0.74.87",
    "@sentry/cli": "^2.24.0",
    "@types/react": "^18.2.6",
    "@types/react-native-background-timer": "^2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "i18next": "^23.16.4",
    "intl-pluralrules": "^2.0.1",
    "jest": "^29.6.3",
    "react-i18next": "^15.0.2",
    "react-native-dotenv": "^3.4.11",
    "typescript": "5.0.4",
    "url": "^0.11.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
